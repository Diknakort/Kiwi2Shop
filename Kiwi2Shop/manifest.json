{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "orderflowclase": {
      "error": "This resource does not support generation in the manifest."
    },
    "maildev": {
      "type": "container.v1",
      "image": "maildev/maildev:latest",
      "bindings": {
        "web": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 1080
        },
        "smtp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 1025
        }
      }
    },
    "postgres": {
      "type": "container.v1",
      "connectionString": "Host={postgres.bindings.tcp.host};Port={postgres.bindings.tcp.port};Username=postgres;Password={postgres-password.value}",
      "image": "docker.io/library/postgres:17.6",
      "volumes": [
        {
          "name": "postgres",
          "target": "/var/lib/postgresql/data",
          "readOnly": false
        }
      ],
      "env": {
        "POSTGRES_HOST_AUTH_METHOD": "scram-sha-256",
        "POSTGRES_INITDB_ARGS": "--auth-host=scram-sha-256 --auth-local=scram-sha-256",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "{postgres-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 5432
        }
      }
    },
    "identitydb": {
      "type": "value.v0",
      "connectionString": "{postgres.connectionString};Database=identitydb"
    },
    "PostgresOrdersDb": {
      "type": "container.v1",
      "connectionString": "Host={PostgresOrdersDb.bindings.tcp.host};Port={PostgresOrdersDb.bindings.tcp.port};Username=postgres;Password={PostgresOrdersDb-password.value}",
      "image": "docker.io/library/postgres:17.6",
      "volumes": [
        {
          "name": "PostgresOrdersDb",
          "target": "/var/lib/postgresql/data",
          "readOnly": false
        }
      ],
      "env": {
        "POSTGRES_HOST_AUTH_METHOD": "scram-sha-256",
        "POSTGRES_INITDB_ARGS": "--auth-host=scram-sha-256 --auth-local=scram-sha-256",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "{PostgresOrdersDb-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 5432
        }
      }
    },
    "OrdersDb": {
      "type": "value.v0",
      "connectionString": "{PostgresOrdersDb.connectionString};Database=OrdersDb"
    },
    "PostgresProductsDb": {
      "type": "container.v1",
      "connectionString": "Host={PostgresProductsDb.bindings.tcp.host};Port={PostgresProductsDb.bindings.tcp.port};Username=postgres;Password={PostgresProductsDb-password.value}",
      "image": "docker.io/library/postgres:17.6",
      "volumes": [
        {
          "name": "PostgresProductsDb",
          "target": "/var/lib/postgresql/data",
          "readOnly": false
        }
      ],
      "env": {
        "POSTGRES_HOST_AUTH_METHOD": "scram-sha-256",
        "POSTGRES_INITDB_ARGS": "--auth-host=scram-sha-256 --auth-local=scram-sha-256",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "{PostgresProductsDb-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 5432
        }
      }
    },
    "ProductsDb": {
      "type": "value.v0",
      "connectionString": "{PostgresProductsDb.connectionString};Database=ProductsDb"
    },
    "rabbitmq": {
      "type": "container.v1",
      "connectionString": "amqp://guest:{rabbitmq-password-uri-encoded.value}@{rabbitmq.bindings.tcp.host}:{rabbitmq.bindings.tcp.port}",
      "image": "docker.io/library/rabbitmq:4.2-management",
      "volumes": [
        {
          "name": "rabbitmq-data",
          "target": "/var/lib/rabbitmq",
          "readOnly": false
        }
      ],
      "env": {
        "RABBITMQ_DEFAULT_USER": "guest",
        "RABBITMQ_DEFAULT_PASS": "{rabbitmq-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 5672
        },
        "management": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 15672
        }
      }
    },
    "redis": {
      "type": "container.v1",
      "connectionString": "{redis.bindings.tcp.host}:{redis.bindings.tcp.port},password={redis-password.value}",
      "image": "docker.io/library/redis:8.2",
      "entrypoint": "/bin/sh",
      "args": [
        "-c",
        "redis-server --requirepass $REDIS_PASSWORD --save 60 1"
      ],
      "volumes": [
        {
          "name": "redis-data-identity",
          "target": "/data",
          "readOnly": false
        }
      ],
      "env": {
        "REDIS_PASSWORD": "{redis-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 6379
        }
      }
    },
    "identity-api": {
      "type": "project.v1",
      "path": "../Kiwi2Shop.identity/Kiwi2Shop.Identity.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{identity-api.bindings.http.targetPort}",
        "ConnectionStrings__rabbitmq": "{rabbitmq.connectionString}",
        "RABBITMQ_HOST": "{rabbitmq.bindings.tcp.host}",
        "RABBITMQ_PORT": "{rabbitmq.bindings.tcp.port}",
        "RABBITMQ_USERNAME": "guest",
        "RABBITMQ_PASSWORD": "{rabbitmq-password.value}",
        "RABBITMQ_URI": "amqp://guest:{rabbitmq-password-uri-encoded.value}@{rabbitmq.bindings.tcp.host}:{rabbitmq.bindings.tcp.port}",
        "ConnectionStrings__identitydb": "{identitydb.connectionString}",
        "IDENTITYDB_HOST": "{postgres.bindings.tcp.host}",
        "IDENTITYDB_PORT": "{postgres.bindings.tcp.port}",
        "IDENTITYDB_USERNAME": "postgres",
        "IDENTITYDB_PASSWORD": "{postgres-password.value}",
        "IDENTITYDB_URI": "postgresql://postgres:{postgres-password-uri-encoded.value}@{postgres.bindings.tcp.host}:{postgres.bindings.tcp.port}/identitydb",
        "IDENTITYDB_JDBCCONNECTIONSTRING": "jdbc:postgresql://{postgres.bindings.tcp.host}:{postgres.bindings.tcp.port}/identitydb",
        "IDENTITYDB_DATABASE": "identitydb",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "identity-api"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "gateway-api": {
      "type": "project.v1",
      "path": "../Kiwi2Shop.ApiGateWay/Kiwi2Shop.GateWay.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{gateway-api.bindings.http.targetPort}",
        "IDENTITY-API_HTTP": "{identity-api.bindings.http.url}",
        "services__identity-api__http__0": "{identity-api.bindings.http.url}",
        "IDENTITY-API_HTTPS": "{identity-api.bindings.https.url}",
        "services__identity-api__https__0": "{identity-api.bindings.https.url}",
        "ConnectionStrings__redis": "{redis.connectionString}",
        "REDIS_HOST": "{redis.bindings.tcp.host}",
        "REDIS_PORT": "{redis.bindings.tcp.port}",
        "REDIS_PASSWORD": "{redis-password.value}",
        "REDIS_URI": "redis://:{redis-password-uri-encoded.value}@{redis.bindings.tcp.host}:{redis.bindings.tcp.port}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "gateway-api"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "kiwi2shop-productsapi": {
      "type": "project.v1",
      "path": "../Kiwi2Shop.ProductsAPI/Kiwi2Shop.ProductsAPI.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{kiwi2shop-productsapi.bindings.http.targetPort}",
        "ConnectionStrings__ProductsDb": "{ProductsDb.connectionString}",
        "PRODUCTSDB_HOST": "{PostgresProductsDb.bindings.tcp.host}",
        "PRODUCTSDB_PORT": "{PostgresProductsDb.bindings.tcp.port}",
        "PRODUCTSDB_USERNAME": "postgres",
        "PRODUCTSDB_PASSWORD": "{PostgresProductsDb-password.value}",
        "PRODUCTSDB_URI": "postgresql://postgres:{PostgresProductsDb-password-uri-encoded.value}@{PostgresProductsDb.bindings.tcp.host}:{PostgresProductsDb.bindings.tcp.port}/ProductsDb",
        "PRODUCTSDB_JDBCCONNECTIONSTRING": "jdbc:postgresql://{PostgresProductsDb.bindings.tcp.host}:{PostgresProductsDb.bindings.tcp.port}/ProductsDb",
        "PRODUCTSDB_DATABASE": "ProductsDb",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "kiwi2shop-productsapi"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "kiwi2shop-ordersapi": {
      "type": "project.v1",
      "path": "../Kiwi2Shop.OrdersAPI/Kiwi2Shop.OrdersAPI.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{kiwi2shop-ordersapi.bindings.http.targetPort}",
        "ConnectionStrings__OrdersDb": "{OrdersDb.connectionString}",
        "ORDERSDB_HOST": "{PostgresOrdersDb.bindings.tcp.host}",
        "ORDERSDB_PORT": "{PostgresOrdersDb.bindings.tcp.port}",
        "ORDERSDB_USERNAME": "postgres",
        "ORDERSDB_PASSWORD": "{PostgresOrdersDb-password.value}",
        "ORDERSDB_URI": "postgresql://postgres:{PostgresOrdersDb-password-uri-encoded.value}@{PostgresOrdersDb.bindings.tcp.host}:{PostgresOrdersDb.bindings.tcp.port}/OrdersDb",
        "ORDERSDB_JDBCCONNECTIONSTRING": "jdbc:postgresql://{PostgresOrdersDb.bindings.tcp.host}:{PostgresOrdersDb.bindings.tcp.port}/OrdersDb",
        "ORDERSDB_DATABASE": "OrdersDb",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "kiwi2shop-ordersapi"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "kiwi2shop-frontend": {
      "type": "container.v1",
      "build": {
        "context": "../kiwi2shop.frontend",
        "dockerfile": "../kiwi2shop.frontend/Dockerfile"
      },
      "env": {
        "NODE_ENV": "development",
        "IDENTITY-API_HTTP": "{identity-api.bindings.http.url}",
        "services__identity-api__http__0": "{identity-api.bindings.http.url}",
        "IDENTITY-API_HTTPS": "{identity-api.bindings.https.url}",
        "services__identity-api__https__0": "{identity-api.bindings.https.url}",
        "VITE_IDENTITY_URL": "{identity-api.bindings.https.url}",
        "PORT": "{kiwi2shop-frontend.bindings.http.targetPort}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "kiwi2shop-frontend"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 8000,
          "external": true
        }
      }
    },
    "kiwi2shop-reverseproxy": {
      "type": "project.v1",
      "path": "../Kiwi2Shop.reverseProxy/Kiwi2Shop.reverseProxy.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{kiwi2shop-reverseproxy.bindings.http.targetPort}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "kiwi2shop-reverseproxy"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "kiwi2shop-notifications": {
      "type": "project.v1",
      "path": "../Kiwi2Shop.Notifications/Kiwi2Shop.Notifications.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ConnectionStrings__rabbitmq": "{rabbitmq.connectionString}",
        "RABBITMQ_HOST": "{rabbitmq.bindings.tcp.host}",
        "RABBITMQ_PORT": "{rabbitmq.bindings.tcp.port}",
        "RABBITMQ_USERNAME": "guest",
        "RABBITMQ_PASSWORD": "{rabbitmq-password.value}",
        "RABBITMQ_URI": "amqp://guest:{rabbitmq-password-uri-encoded.value}@{rabbitmq.bindings.tcp.host}:{rabbitmq.bindings.tcp.port}",
        "OTEL_EXPORTER_OTLP_ENDPOINT": "{orderflowclase-dashboard.bindings.otlp-grpc.url}",
        "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
        "OTEL_SERVICE_NAME": "kiwi2shop-notifications"
      }
    },
    "postgres-password": {
      "type": "parameter.v0",
      "value": "{postgres-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22
            }
          }
        }
      }
    },
    "postgres-password-uri-encoded": {
      "type": "annotated.string",
      "value": "{postgres-password.value}",
      "filter": "uri"
    },
    "PostgresOrdersDb-password": {
      "type": "parameter.v0",
      "value": "{PostgresOrdersDb-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22
            }
          }
        }
      }
    },
    "PostgresOrdersDb-password-uri-encoded": {
      "type": "annotated.string",
      "value": "{PostgresOrdersDb-password.value}",
      "filter": "uri"
    },
    "PostgresProductsDb-password": {
      "type": "parameter.v0",
      "value": "{PostgresProductsDb-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22
            }
          }
        }
      }
    },
    "PostgresProductsDb-password-uri-encoded": {
      "type": "annotated.string",
      "value": "{PostgresProductsDb-password.value}",
      "filter": "uri"
    },
    "rabbitmq-password": {
      "type": "parameter.v0",
      "value": "{rabbitmq-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22,
              "special": false
            }
          }
        }
      }
    },
    "rabbitmq-password-uri-encoded": {
      "type": "annotated.string",
      "value": "{rabbitmq-password.value}",
      "filter": "uri"
    },
    "redis-password": {
      "type": "parameter.v0",
      "value": "{redis-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22,
              "special": false
            }
          }
        }
      }
    },
    "redis-password-uri-encoded": {
      "type": "annotated.string",
      "value": "{redis-password.value}",
      "filter": "uri"
    },
    "orderflowclase-dashboard": {
      "type": "container.v1",
      "image": "mcr.microsoft.com/dotnet/nightly/aspire-dashboard:latest",
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 18888
        },
        "otlp-grpc": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 18889
        }
      }
    }
  }
}